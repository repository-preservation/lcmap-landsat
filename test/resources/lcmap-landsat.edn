{:database  {:cluster {:contact-points "localhost"}
             :default-keyspace "lcmap_landsat_unit"}
 :event     {:host "localhost"
             :port 5672
             :queues [{:name "unit.lcmap.landsat.server"
                       :opts {:durable true
                              :exclusive false
                              :auto-delete false}}
                      {:name "unit.lcmap.landsat.worker"
                       :opts {:durable true
                              :exclusive false
                              :auto-delete false}}]
             :exchanges [{:name "unit.lcmap.landsat.server"
                          :type "topic"
                          :opts {:durable true}}
                         {:name "unit.lcmap.landsat.worker"
                          :type "topic"
                          :opts {:durable true}}]
             :bindings [{:exchange "unit.lcmap.landsat.server"
                         :queue "unit.lcmap.landsat.worker"
                         :opts {:routing-key "ingest"}}]}
 :http      {:port 5679
             :join? false
             :daemon? true}
 :server    {:exchange "unit.lcmap.landsat.server"
             :queue    "unit.lcmap.landsat.server"}
 :worker    {:exchange "unit.lcmap.landsat.worker"
             :queue    "unit.lcmap.landsat.worker"}
 :search     {:index-url      "http://localhost:9200/tile-specs"
              :refresh-url    "http://localhost:9200/tile-specs/_refresh"
              :bulk-api-url   "http://localhost:9200/tile-specs/unit/_bulk"
              :search-api-url "http://localhost:9200/tile-specs/_search"
              :max-result-size 10000}}
