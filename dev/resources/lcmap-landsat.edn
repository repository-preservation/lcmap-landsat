{
  :database  {:cluster {:contact-points "localhost"
                        :socket-options {:read-timeout-millis 20000}}
              :default-keyspace "lcmap_landsat_local"}
  :event     {
              :host "localhost"
              :port 5672
              :queues [{:name "local.lcmap.landsat.server"
                        :opts {:durable true
                               :exclusive false
                               :auto-delete false}}
                       {:name "local.lcmap.landsat.worker"
                        :opts {:durable true
                               :exclusive false
                               :auto-delete false}}]
              :exchanges [{:name "local.lcmap.landsat.server"
                           :type "topic"
                           :opts {:durable true}}
                          {:name "local.lcmap.landsat.worker"
                           :type "topic"
                           :opts {:durable true}}]
              :bindings [{:exchange "local.lcmap.landsat.server"
                          :queue "local.lcmap.landsat.worker"
                          :opts {:routing-key "ingest"}}]}
 :http      {:port 5678
             :join? false
             :daemon? true}
 :server    {:exchange "local.lcmap.landsat.server"
             :queue "local.lcmap.landsat.server"}
 :worker    {:exchange "local.lcmap.landsat.worker"
             :queue    "local.lcmap.landsat.worker"}
 :search     {:index-url      "http://localhost:9200/tile-specs"
              :refresh-url    "http://localhost:9200/tile-specs/_refresh"
              :bulk-api-url   "http://localhost:9200/tile-specs/local/_bulk"
              :search-api-url "http://localhost:9200/tile-specs/_search"
              :max-result-size 10000}}
